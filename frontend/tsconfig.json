{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "module": "esnext",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@icons": [
        "icons/"
      ],
      "*": [
        "node_modules/*"
      ]
    }
  },
}