{
   "type": "sqlite",
   "logging": false,
   "database": "database.sqlite",
   "synchronize": false,
   "entities": [
      "src/models/**/*{.ts,.js}"
   ],
   "migrations": [
      "src/migration/**/*{.ts,.js}"
   ],
   "subscribers": [
      "src/subscriber/**/*{.ts,.js}"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}
